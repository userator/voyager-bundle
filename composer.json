    "description": "graphql voyager bundle",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "userator",
            "email": "userator@ya.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/http-kernel": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/config": "^5.4",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
